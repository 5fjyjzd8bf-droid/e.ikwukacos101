fn main() {
    let sum = 5550+7310;
    println!("The addition of 5550 and 7310 is {}",sum);

    let difference:f32 = 95.5-4.3;
    println!("the difference of 95.5 and 4.3 is{}",difference );

    let product:i32 = 4 * 30;
    println!("the product of multiplying 4 and 30 is {}",product );

    let quotient:f32 = 56.7/32.2 ;
    println!("the division on 56.7 by 32.2 is {}",quotient );

    let remainder:i32 =43%5;
    print!("the remainder when 43 is divided by 5 is {}",remainder );
}
